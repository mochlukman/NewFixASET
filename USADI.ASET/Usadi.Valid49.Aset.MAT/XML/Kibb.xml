    <typeAlias alias="Kibb" type="Usadi.Valid49.BO.KibbControl, Usadi.Valid49.Aset.MAT" />
  </alias>
  <resultMaps>
    <resultMap id="return-result" class="Kibb">
      <result property="Id" column="ID"/>
      <result property="Unitkey" column="UNITKEY"/>
