      <result property="Kdpemilik" column="KDPEMILIK"/>
      <result property="Nmpemilik" column="NMPEMILIK"/>
      <result property="Asalusul" column="ASALUSUL"/>
      <result property="Pengguna" column="PENGGUNA"/>
      <result property="Kdkon" column="KDKON"/>
