<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="Kibedet" xmlns="http://ibatis.apache.org/mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <alias>
    <typeAlias alias="Kibedet" type="Usadi.Valid49.BO.KibedetControl, Usadi.Valid49.Aset.MAT" />
  </alias>
  <resultMaps>
    <resultMap id="return-result" class="Kibedet">
      <result property="Id" column="ID"/>
      <result property="Unitkey" column="UNITKEY"/>
      <result property="Kdunit" column="KDUNIT"/>
      <result property="Nmunit" column="NMUNIT"/>
