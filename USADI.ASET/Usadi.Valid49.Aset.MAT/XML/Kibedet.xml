      <result property="Uruttrans" column="URUTTRANS"/>
    </resultMap>
  </resultMaps>
  <statements>
    <insert id="InsertKibedet" parameterMap="insert-param">
      <generate table="Aset_kibedet"/>
    </insert>
    <update id="UpdateByKibedet" parameterMap="update-param">
      <generate table="Aset_kibedet" by="IDBRG,URUTTRANS"/>
    </update>
    <update id="UpdateByValidRevKibedet" parameterClass="Kibedet">
      update ASET_KIBEDET set REV_BY=#Rev_by# and REV_DATE=#Rev_date#
    </update>
    <update id="UpdateByUnValidRevKibedet" parameterClass="Kibedet">
      update ASET_KIBEDET set REV_BY='' and REV_DATE=null
    </update>
    <delete id="DeleteByKibedet" parameterMap="delete-param">
      <generate table="Aset_kibedet" by="IDBRG,URUTTRANS"/>
    </delete>
    <select id="BaseQueryKibedet" parameterClass="Kibedet" resultMap="return-result">
      select A.ID, A.IDBRG, A.URUTTRANS, A.UNITKEY, D.KDUNIT, D.NMUNIT, A.ASETKEY, A.TAHUN, A.NOREG, A.NODOKUMEN, A.TGLDOKUMEN, A.NILAI
      , A.UMEKO, A.KDTANS, J.NMTRANS, A.KDKIB, A.KET, A.KDKON, A.KDKLAS, A.KDSENSUS, A.KDSTATUSASET, A.KDLOKPO, A.KDBRGPO, A.THANG
      , CASE WHEN LEFT(A.KDTANS,1) IN('0','1') THEN J.NMTRANS+' (+)' WHEN LEFT(A.KDTANS,1)='2' THEN J.NMTRANS+' (-)' ELSE J.NMTRANS  END AS URAITRANS
      , CASE WHEN LEFT(A.KDTANS,1) IN('0','1','3') THEN A.NILAI WHEN LEFT(A.KDTANS,1)='2' THEN -1*A.NILAI END AS NILAITRANS
      from ASET_KIBEDET A
      left join DAFTUNIT D on D.UNITKEY=A.UNITKEY
      left join JTRANS J on J.KDTANS=A.KDTANS
    </select>
    <select id="QueryByAllKibedet" extends="BaseQueryKibedet" parameterClass="Kibedet" resultMap="return-result">
      where 1=1
      and A.IDBRG=#Idbrg#
      and A.UNITKEY=#Unitkey#
      and A.ASETKEY=#Asetkey#
      and A.KDTANS NOT IN ('201','204')
      ORDER BY A.TGLDOKUMEN,A.NODOKUMEN
    </select>
  </statements>
  <parameterMaps>
    <parameterMap id="insert-param" class="Kibedet">
