  <statements>
    <select id="BaseQueryKibkemitraandet" parameterClass="Kibkemitraandet" resultMap="return-result">
      select A.ID, A.IDBRG, A.URUTTRANS, A.UNITKEY, D.KDUNIT, D.NMUNIT, A.ASETKEY, A.TAHUN, A.NOREG, A.NODOKUMEN, A.TGLDOKUMEN, A.NILAI
      , A.UMEKO, A.KDTANS, J.NMTRANS, A.KDKIB, A.KET, A.KDKON, A.KDKLAS, A.KDSENSUS, A.KDSTATUSASET, A.KDLOKPO, A.KDBRGPO, A.THANG
      , CASE WHEN LEFT(A.KDTANS,1) IN('0','1') THEN J.NMTRANS+' (+)' WHEN LEFT(A.KDTANS,1)='2' THEN J.NMTRANS+' (-)' ELSE J.NMTRANS  END AS URAITRANS
      , CASE WHEN LEFT(A.KDTANS,1) IN('0','1','3') THEN A.NILAI WHEN LEFT(A.KDTANS,1)='2' THEN -1*A.NILAI END AS NILAITRANS
      from (
      select * from ASET_KIBADET A where A.IDBRG=#Idbrg# and A.UNITKEY=#Unitkey# and A.ASETKEY=#Asetkey#
      union all
      select * from ASET_KIBBDET A where A.IDBRG=#Idbrg# and A.UNITKEY=#Unitkey# and A.ASETKEY=#Asetkey#
      union all
      select * from ASET_KIBCDET A where A.IDBRG=#Idbrg# and A.UNITKEY=#Unitkey# and A.ASETKEY=#Asetkey#
      union all
      select * from ASET_KIBDDET A where A.IDBRG=#Idbrg# and A.UNITKEY=#Unitkey# and A.ASETKEY=#Asetkey#
      union all
      select * from ASET_KIBEDET A where A.IDBRG=#Idbrg# and A.UNITKEY=#Unitkey# and A.ASETKEY=#Asetkey#
      union all
      select * from ASET_KIBGDET A where A.IDBRG=#Idbrg# and A.UNITKEY=#Unitkey# and A.ASETKEY=#Asetkey#
      ) A
      left join DAFTUNIT D on D.UNITKEY=A.UNITKEY
      left join JTRANS J on J.KDTANS=A.KDTANS
    </select>
    <select id="QueryByAllKibkemitraandet" extends="BaseQueryKibkemitraandet" parameterClass="Kibkemitraandet" resultMap="return-result">
      where 1=1
    </select>
  </statements>
